{
  "name": "md-element-plus",
  "version": "1.0.2",
  "description": "Components library by Vue3 + TS",
  "type": "module",
  "main": "./dist/umd/index.umd.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/core/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.umd.cjs",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./dist/index.css"
  ],
  "scripts": {
    "build": "run-s clean build-only move-style",
    "build-only": "run-p build-es build-umd",
    "build-umd": "vite build --config ./build/vite.umd.config.ts",
    "build-es": "vite build --config ./build/vite.es.config.ts",
    "move-style": "move-file dist/es/index.css dist/index.css",
    "clean": "rimraf dist",
    "release": "release-it"
  },
  "dependencies": {},
  "devDependencies": {
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
